{
    "params_name": "LSTUT_FELIX_TRIAL",
    "num_epochs": 15,
    "early_stopping_patience": 20,
    "num_examples_to_save": 3,
    "save_model_every": 100,
    "save_img_every": 1,
    "max_time_minutes": 10,

    "seq_length": 32,
    "padding_amt": 16,
    "lr": 0.0005,
    "batch_size": 32,
    "clip_gradient_norm": 0.5,
    "num_feats": 1,
    "dataset_proportion": 0.04,

    "dset_path": "processed_datasets/all_string_quartets_agnostic_byline.h5",
    "dset_testing_path": "processed_datasets/supervised_omr_targets_byline.h5",
    "error_model": "data_augmentation/quartet_omr_error_models_byline.joblib",
    "saved_vocabulary": "data_management/vocab.txt",
    "error_gen_smoothing": 10,
    "simple_errors": false,
    "errors_parallel": 1,
    "simple_error_rate": 0.05,
    "target_recalls": [0.2, 0.5, 0.8],

    "lstut_settings": {
        "num_feats": 1,
        "output_feats": 1,
        "lstm_layers": 2,
        "tf_layers": 0,
        "tf_heads": 1,
        "tf_depth": 0,
        "hidden_dim": 32,
        "ff_dim": 64,
        "dropout": 0.1,
        "positional_encoding": false
    },
    "scheduler_settings": {
        "factor": 0.25,
        "patience": 6,
        "threshold": 0.001,
        "verbose": true
    },
    "param_sweep":
    [
        {"batch_size": 10, "seq_length": 32},
        {"lstut_settings.tf_depth": 3},
        {"lstut_settings.tf_depth": 2, "seq_length": 33}
    ]
}
