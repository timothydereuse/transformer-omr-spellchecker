{
    "params_name": "LSTUT_TRIAL",
    "num_epochs": 2,
    "early_stopping_patience": 20,
    "num_examples_to_save": 3,
    "save_model_every": 100,
    "save_img_every": 1,
    "max_time_minutes": 2,

    "seq_length": 64,
    "padding_amt": 16,
    "lr": 0.0005,
    "batch_size": 32,
    "clip_gradient_norm": 0.5,
    "num_feats": 1,
    "dataset_proportion": 0.01,

    "dset_path": "processed_datasets/all_string_quartets_agnostic_byline.h5",
    "dset_testing_path": "processed_datasets/supervised_omr_targets_byline.h5",
    "error_model": "processed_datasets/quartet_omr_error_models_byline.joblib",
    "saved_vocabulary": "data_management/vocab_big.txt",
    "error_gen_smoothing": 2,
    "simple_errors": false,
    "errors_parallel": 1,
    "simple_error_rate": 0.05,
    "target_recalls": [0.8, 0.9, 0.99],

    "lstut_settings": {
        "num_feats": 1,
        "output_feats": 1,
        "lstm_layers": 1,
        "tf_layers": 1,
        "tf_heads": 1,
        "tf_depth": 1,
        "hidden_dim": 32,
        "ff_dim": 32,
        "dropout": 0.1,
        "positional_encoding": false
    },
    "scheduler_settings": {
        "factor": 0.25,
        "patience": 6,
        "threshold": 0.001,
        "verbose": true
    },
    "param_sweep":
    [
        {"batch_size": 10, "seq_length": 32},
        {"lstut_settings.tf_depth": 3},
        {"lstut_settings.tf_depth": 2, "seq_length": 33}
    ],
    "test_sets":
    [
        {"base": "omr", "with_targets": true},
        {"base": "onepass", "with_targets": true},
        {"base": "test", "with_targets": false}
    ]
}
