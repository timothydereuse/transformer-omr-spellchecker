{
    "params_name": "LSTUT_FELIX_TRIAL",
    "num_epochs": 50,
    "early_stopping_patience": 20,
    "save_model_every": 100,
    "save_img_every": 1,
    "max_time_minutes": 30,

    "seq_length": 64,
    "padding_amt": 16,
    "lr": 0.0005,
    "batch_size": 32,
    "clip_gradient_norm": 0.5,
    "num_feats": 4,
    "dataset_proportion": 0.02,

    "dset_path": "processed_datasets/all_string_quartets_agnostic.h5",
    "error_model": "data_augmentation/quartet_omr_error_models.joblib",
    "saved_vocabulary": "data_management/vocab.txt",
    "error_gen_smoothing": 0.7,
    "simple_errors": false,
    "errors_parallel": 2,
    "simple_error_rate": 0.05,

    "lstut_settings": {
        "num_feats": 1,
        "output_feats": 1,
        "lstm_layers": 0,
        "tf_layers": 1,
        "tf_heads": 1,
        "tf_depth": 2,
        "hidden_dim": 64,
        "ff_dim": 32,
        "dropout": 0.1,
        "positional_encoding": true
    },
    "scheduler_settings": {
        "factor": 0.25,
        "patience": 5,
        "threshold": 0.001,
        "verbose": true
    },
    "parameter_searching":{
        "lstut_settings.tf_depth": [1, 3, 5]
    }
}
