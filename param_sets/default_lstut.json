{
    "params_name": "LSTUT_GPU_TRIAL",
    "num_epochs": 500,
    "early_stopping_patience": 30,
    "num_examples_to_save": 3,
    "save_model_every": 100,
    "save_img_every": 1,
    "max_time_minutes": 40,
    "seq_length": 128,
    "padding_amt": 16,
    "lr": 0.0005,
    "batch_size": 512,
    "clip_gradient_norm": 0.5,
    "minibatch_div": 100,
    "aug_dset_path": "./processed_datasets/all_quartets_bymeasure.h5",
    "dset_path": "processed_datasets/supervised_omr_targets_bymeasure.h5",
    "error_model": "./processed_datasets/paired_quartets_error_model_bymeasure.joblib",
    "saved_vocabulary": "./processed_datasets/vocab_big.txt",
    "error_gen_smoothing": 2,
    "simple_errors": false,
    "errors_parallel": 1,
    "simple_error_rate": 0.05,
    "target_recalls": [
        0.8,
        0.9,
        0.99
    ],
    "use_mcc_loss": false,
    "lstut_settings": {
        "output_feats": 1,
        "lstm_layers": 1,
        "d_model": 128,
        "tf_layers": 1,
        "tf_heads": 6,
        "tf_depth": 1,
        "hidden_dim": 64,
        "ff_dim": 256,
        "dropout": 0.1,
        "positional_encoding": false
    },
    "scheduler_settings": {
        "max_lr": 0.1,
        "step_size_up": 8,
        "verbose": true
    },
    "test_sets": [
        {
            "base": "omr",
            "with_targets": true
        },
        {
            "base": "onepass",
            "with_targets": true
        },
        {
            "base": "test",
            "with_targets": false
        }
    ],
    "param_sweep": [
        {
            "batch_size": 10,
            "seq_length": 32
        },
        {
            "lstut_settings.tf_depth": 3
        },
        {
            "lstut_settings.tf_depth": 2,
            "seq_length": 33
        }
    ]
}